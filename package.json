{
  "name": "backend",
  "version": "0.0.1",
  "description": "API responsible for external integrations needed during conversation.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -r dotenv/config",
    "test": "jest --setupFiles dotenv/config",
    "lint": "eslint src/*.js index.js server.js --fix"
  },
  "keywords": [
    "bot",
    "api"
  ],
  "author": "Lucas Braganca",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "yup": "^0.28.0",
    "lodash": "latest"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.25",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
